  "keywords": [
    "mcp",
    "model-context-protocol",
    "claude",
    "cursor",
    "ai",
    "context",
    "search",
    "memory",
    "vector-search",
    "typescript",
    "server",
    "archivenet"
  ],
  "author": "Your Name <your.email@example.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/your-username/archivenet.git"
  },
  "bugs": {
    "url": "https://github.com/your-username/archivenet/issues"
  },
  "homepage": "https://github.com/your-username/archivenet#readme",
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "^0.5.0",
    "dotenv": "^16.3.1",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "tsx": "^4.6.0",
    "typescript": "^5.3.0"
  }
}